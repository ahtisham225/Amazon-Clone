port const reducer;","/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Home.js",[],[],"/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Login.js",[],[],"/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Checkout.js",["73"],[],"import React from 'react';\nimport {useStateValue} from './StateProvider'\nimport Header from './Header';\nimport './Checkout.css';\nimport CheckoutProduct from './CheckoutProduct';\nimport Subtotal from './Subtotal'\nfunction Checkout(){\n    const [{basket}] = useStateValue();\n    return <div>\n        <Header />\n        <div className='checkout'>\n        <div className='checkout_left'>       \n            {/* <img src='addBanner.png'></img>      */}\n            <img className=\"checkout_image\" src='https://i.ibb.co/6rmJPHd/add-Banner.png'></img>\n            <h1 className='header_title'>Checkout</h1>\n            {basket?.length === 0 ? (\n                <div>\n                    <h2> Your Shopping Basket is empty </h2>\n                </div>\n            ):(\n                <div>\n                    <h2> Your Shopping Basket </h2>\n                    {basket?.map(item =>(\n                        <CheckoutProduct\n                        id = {item.id}\n                        title = {item.title}\n                        image = {item.image}\n                        price = {item.price}\n                        rating = {item.rating}\n                        />\n                    ))}\n                </div>\n                )}\n        </div>\n        \n            {basket.length > 0 &&(\n                <div className='subtotal_right'>\n                    <Subtotal />\n                </div>)}\n    </div>\n    </div>\n}\n\nexport default Checkout;","/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Header.js",[],[],"/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Footer.js",[],[],"/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/CheckoutProduct.js",["74","75"],[],"import { ListItem } from '@mui/material';\nimport React from 'react';\nimport './CheckoutProduct.css';\nimport { useStateValue } from './StateProvider';\n\nfunction CheckoutProduct({id, title, image, price, rating}){\n    const  [{},dispatch] = useStateValue();\n    function removeFromBasket(){\n        dispatch({\n            type: 'REMOVE_FROM_BASKET',\n            id: id,\n        })\n    }\n    return (\n        <div className='checkoutProduct'>\n            <img className='checkoutProduct_image' src = {image} alt=''/>\n            <div className='checkoutProduct_info'>\n                <p className='checkoutProduct_title'>{title}</p>\n                <p className='checkoutProduct_price'> $ {price}</p>\n                <div className='checkoutProduct_rating'>\n                    {Array(rating).fill().map((_,i)=>(\n                        <p>üíé</p>\n                    ))}\n                </div>\n                <button onClick={removeFromBasket}>Remove From Basket</button>\n            </div>\n        </div>\n    )\n}\n\nexport default CheckoutProduct;","/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Subtotal.js",["76"],[],"import React from 'react';\nimport './Subtotal.css';\nimport CurrencyFormat from 'react-currency-format';\nimport { useStateValue } from './StateProvider';\nimport { getBasketTotal } from './reducer';\n\nfunction Subtotal(){\n    const [{basket},dispatch] = useStateValue();\n    return <div className='SubTotal'>\n        <CurrencyFormat \n            renderText={(value) => (\n                <>\n                    <p>\n                        Subtotal ({basket.length} items): <strong>{value}</strong>\n                    </p>\n                </>\n            )}\n            decimalScale={2}\n            value={getBasketTotal(basket)}\n            displayType ={\"text\"}\n            thousandSeparator={true}\n            prefix ={\"$\"}\n        />\n        <button className='subTotalButton'>Proceed to CheckOut</button> \n    </div> \n}\n\nexport default Subtotal;","/home/ahtisham/Desktop/Jobs/Amazon Clone/Amazon-Clone/Amazon-Clone/amazon-clone/src/Product.js",["77","78"],[],"import React from \"react\";\nimport './Product.css'\nimport { useStateValue } from './StateProvider'\n\nfunction Product({id, title, image, price, rating}){\n\n    const  [{},dispatch] = useStateValue();\n\n    const addToBasket = () => {\n        // console.log('coming here 1')\n        dispatch({\n            type: 'ADD_TO_BASKET',\n            item: {\n                id: id,\n                title: title,\n                image: image,\n                price: price,\n                rating: rating,\n            }\n        })\n    };\n\n    return (\n        <div className=\"product\">\n            {/* <div className=\"product_image\"> */}\n            <img className=\"product_image\" src={image}></img>\n            {/* </div> */}\n            <p className=\"nameOfProduct\">{title}</p>\n            <p className=\"product_price\">\n                <strong> $$ </strong>\n                <strong>{price}</strong>\n            </p>\n            <div className=\"product_rating\">\n                {\n                    Array(rating).fill().map((_)=>(\n                        <p>‚≠ê</p>\n                    ))}\n            </div>\n            <button onClick={addToBasket}>Add to Basket</button>\n        </div>\n    )\n}\nexport default Product;",{"ruleId":"79","severity":1,"message":"80","line":32,"column":13,"nodeType":"81","messageId":"82","endLine":32,"endColumn":19},{"ruleId":"83","severity":1,"message":"84","line":14,"column":13,"nodeType":"85","endLine":14,"endColumn":91},{"ruleId":"86","severity":1,"message":"87","line":1,"column":10,"nodeType":"88","messageId":"89","endLine":1,"endColumn":18},{"ruleId":"90","severity":1,"message":"91","li